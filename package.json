{
  "name": "miragejs",
  "version": "0.1.48",
  "description": "A client-side server to help you build, test and demo your JavaScript app",
  "sideEffects": false,
  "exports": {
    ".": {
      "import": {
        "types": "./types/index.d.ts",
        "default": "./dist/mirage.mjs"
      },
      "require": {
        "types": "./types/index.d.ts",
        "default": "./dist/mirage.js"
      }
    },
    "./package.json": "./package.json"
  },
  "keywords": [
    "pretender",
    "prototype",
    "server",
    "testing"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/miragejs/miragejs.git"
  },
  "license": "MIT",
  "author": "Sam Selikoff",
  "homepage": "https://github.com/miragejs/miragejs",
  "bugs": {
    "url": "https://github.com/miragejs/miragejs/issues"
  },
  "scripts": {
    "lint": "eslint .",
    "prettier:check": "prettier --list-different '**/*{.js,.ts}'",
    "prettier:update": "prettier --write .",
    "dev": "rollup -c -w",
    "build": "rollup -c",
    "prepublishOnly": "yarn run build",
    "test:run": "jest",
    "test": "run-s build test:run",
    "ci": "run-s lint prettier:check test"
  },
  "dependencies": {
    "@miragejs/pretender-node-polyfill": "^0.1.0",
    "inflected": "^2.0.4",
    "lodash": "^4.0.0",
    "pretender": "^3.4.7"
  },
  "devDependencies": {
    "@babel/core": "^7.24.7",
    "@babel/eslint-parser": "^7.24.7",
    "@babel/preset-env": "^7.24.7",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "babel-jest": "^29.7.0",
    "core-js": "^3.37.1",
    "eslint": "^8.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-import-resolver-node": "^0.3.9",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jest": "^28.6.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-extended": "^4.0.2",
    "npm-run-all": "^4.1.5",
    "prettier": "3.3.2",
    "rollup": "^4.18.0",
    "@rollup/plugin-alias": "^5.1.0",
    "@rollup/plugin-commonjs": "26.0.1",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^5.4.5"
  },
  "packageManager": "yarn@1.22.19+sha1.4ba7fc5c6e704fce2066ecbfb0b0d8976fe62447"
}
